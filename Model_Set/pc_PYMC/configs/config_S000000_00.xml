<PhysiCell_settings version="devel-version">

    <domain>
        <x_min>-500</x_min>
        <x_max>500</x_max>
        <y_min>-500</y_min>
        <y_max>500</y_max>
        <z_min>-10</z_min>
        <z_max>10</z_max>
        <dx>20</dx>
        <dy>20</dy>
        <dz>20</dz>
        <use_2D>true</use_2D>
    </domain>

    <overall>
        <max_time units="min">2880</max_time>
        <time_units>min</time_units>
        <space_units>micron</space_units>
        <dt_diffusion units="min">0.01</dt_diffusion>
        <dt_mechanics units="min">0.1</dt_mechanics>
        <dt_phenotype units="min">6</dt_phenotype>
    </overall>

    <parallel>
        <omp_num_threads>8</omp_num_threads>
    </parallel>

    <save>
        <folder>./Model_Set/pc_PYMC/output/Sim_000000_00/</folder>
        <full_data>
            <interval units="min">60</interval>
            <enable>false</enable>
        </full_data>
        <SVG>
            <interval units="min">60</interval>
            <enable>false</enable>
            <plot_substrate enabled="false" limits="false">
                <substrate>drug1</substrate>
                <min_conc />
                <max_conc />
            </plot_substrate>
        </SVG>
        <legacy_data>
            <enable>false</enable>
        </legacy_data>
    </save>

    <options>
        <legacy_random_points_on_sphere_in_divide>false</legacy_random_points_on_sphere_in_divide>
        <virtual_wall_at_domain_edge>true</virtual_wall_at_domain_edge>
        <disable_automated_spring_adhesions>false</disable_automated_spring_adhesions>
    </options>

    <microenvironment_setup>
        <variable name="drug1" units="dimensionless" ID="0">
            <physical_parameter_set>
                <diffusion_coefficient units="micron^2/min">10e6</diffusion_coefficient>
                <decay_rate units="1/min">0</decay_rate>
            </physical_parameter_set>
            <initial_condition units="dimensionless">1</initial_condition>
            <Dirichlet_boundary_condition units="dimensionless" enabled="False">0</Dirichlet_boundary_condition>
            <Dirichlet_options>
                <boundary_value ID="xmin" enabled="False">0</boundary_value>
                <boundary_value ID="xmax" enabled="False">0</boundary_value>
                <boundary_value ID="ymin" enabled="False">0</boundary_value>
                <boundary_value ID="ymax" enabled="False">0</boundary_value>
                <boundary_value ID="zmin" enabled="False">0</boundary_value>
                <boundary_value ID="zmax" enabled="False">0</boundary_value>
            </Dirichlet_options>
        </variable>
        <variable name="drug2" units="dimensionless" ID="1">
            <physical_parameter_set>
                <diffusion_coefficient units="micron^2/min">25600.0</diffusion_coefficient>
                <decay_rate units="1/min">0</decay_rate>
            </physical_parameter_set>
            <initial_condition units="dimensionless">0</initial_condition>
            <Dirichlet_boundary_condition units="dimensionless" enabled="False">0</Dirichlet_boundary_condition>
            <Dirichlet_options>
                <boundary_value ID="xmin" enabled="False">0</boundary_value>
                <boundary_value ID="xmax" enabled="False">0</boundary_value>
                <boundary_value ID="ymin" enabled="False">0</boundary_value>
                <boundary_value ID="ymax" enabled="False">0</boundary_value>
                <boundary_value ID="zmin" enabled="False">0</boundary_value>
                <boundary_value ID="zmax" enabled="False">0</boundary_value>
            </Dirichlet_options>
        </variable>
        <options>
            <calculate_gradients>true</calculate_gradients>
            <track_internalized_substrates_in_each_agent>true</track_internalized_substrates_in_each_agent>
            <initial_condition enabled="false" type="matlab">
                <filename>./config/initial.mat</filename>
            </initial_condition>
            <dirichlet_nodes enabled="false" type="matlab">
                <filename>./config/dirichlet.mat</filename>
            </dirichlet_nodes>
        </options>
    </microenvironment_setup>

    <cell_definitions>
        <cell_definition name="cancer" ID="0">
            <phenotype>
                <cycle code="6" name="Flow cytometry model (separated)">
                    <phase_durations units="min">
                        <duration index="0" fixed_duration="false">300.0</duration>
                        <duration index="1" fixed_duration="false">480</duration>
                        <duration index="2" fixed_duration="false">240</duration>
                        <duration index="3" fixed_duration="false">60</duration>
                    </phase_durations>
                </cycle>
                <death>
                    <model code="100" name="apoptosis">
                        <death_rate units="1/min">5.31667e-05</death_rate>
                        <phase_durations units="min">
                            <duration index="0" fixed_duration="true">516</duration>
                        </phase_durations>
                        <parameters>
                            <unlysed_fluid_change_rate units="1/min">0.05</unlysed_fluid_change_rate>
                            <lysed_fluid_change_rate units="1/min">0</lysed_fluid_change_rate>
                            <cytoplasmic_biomass_change_rate units="1/min">1.66667e-02</cytoplasmic_biomass_change_rate>
                            <nuclear_biomass_change_rate units="1/min">5.83333e-03</nuclear_biomass_change_rate>
                            <calcification_rate units="1/min">0</calcification_rate>
                            <relative_rupture_volume units="dimensionless">2.0</relative_rupture_volume>
                        </parameters>
                    </model>
                    <model code="101" name="necrosis">
                        <death_rate units="1/min">0.0</death_rate>
                        <phase_durations units="min">
                            <duration index="0" fixed_duration="true">0</duration>
                            <duration index="1" fixed_duration="true">86400</duration>
                        </phase_durations>
                        <parameters>
                            <unlysed_fluid_change_rate units="1/min">0.05</unlysed_fluid_change_rate>
                            <lysed_fluid_change_rate units="1/min">0</lysed_fluid_change_rate>
                            <cytoplasmic_biomass_change_rate units="1/min">1.66667e-02</cytoplasmic_biomass_change_rate>
                            <nuclear_biomass_change_rate units="1/min">5.83333e-03</nuclear_biomass_change_rate>
                            <calcification_rate units="1/min">0</calcification_rate>
                            <relative_rupture_volume units="dimensionless">2.0</relative_rupture_volume>
                        </parameters>
                    </model>
                </death>
                <volume>
                    <total units="micron^3">2494</total>
                    <fluid_fraction units="dimensionless">0.75</fluid_fraction>
                    <nuclear units="micron^3">540</nuclear>
                    <fluid_change_rate units="1/min">0.05</fluid_change_rate>
                    <cytoplasmic_biomass_change_rate units="1/min">0.0045</cytoplasmic_biomass_change_rate>
                    <nuclear_biomass_change_rate units="1/min">0.0055</nuclear_biomass_change_rate>
                    <calcified_fraction units="dimensionless">0</calcified_fraction>
                    <calcification_rate units="1/min">0</calcification_rate>
                    <relative_rupture_volume units="dimensionless">2.0</relative_rupture_volume>
                </volume>
                <mechanics>
                    <cell_cell_adhesion_strength units="micron/min">0.4</cell_cell_adhesion_strength>
                    <cell_cell_repulsion_strength units="micron/min">10.0</cell_cell_repulsion_strength>
                    <relative_maximum_adhesion_distance units="dimensionless">1.25</relative_maximum_adhesion_distance>
                    <cell_adhesion_affinities>
                        <cell_adhesion_affinity name="default">1</cell_adhesion_affinity>
                        <cell_adhesion_affinity name="cancer">1.0</cell_adhesion_affinity>
                    </cell_adhesion_affinities>
                    <options>
                        <set_relative_equilibrium_distance enabled="false" units="dimensionless">1.8</set_relative_equilibrium_distance>
                        <set_absolute_equilibrium_distance enabled="false" units="micron">15.12</set_absolute_equilibrium_distance>
                    </options>
                    <cell_BM_adhesion_strength units="micron/min">4.0</cell_BM_adhesion_strength>
                    <cell_BM_repulsion_strength units="micron/min">10.0</cell_BM_repulsion_strength>
                    <attachment_elastic_constant units="1/min">0.0</attachment_elastic_constant>
                    <attachment_rate units="1/min">0.0</attachment_rate>
                    <detachment_rate units="1/min">0.0</detachment_rate>
                </mechanics>
                <motility>
                    <speed units="micron/min">1</speed>
                    <persistence_time units="min">1</persistence_time>
                    <migration_bias units="dimensionless">.5</migration_bias>
                    <options>
                        <enabled>false</enabled>
                        <use_2D>true</use_2D>
                        <chemotaxis>
                            <enabled>false</enabled>
                            <substrate>drug1</substrate>
                            <direction>1</direction>
                        </chemotaxis>
                        <advanced_chemotaxis>
                            <enabled>false</enabled>
                            <normalize_each_gradient>false</normalize_each_gradient>
                            <chemotactic_sensitivities>
                                <chemotactic_sensitivity substrate="drug1">0.0</chemotactic_sensitivity>
                                <chemotactic_sensitivity substrate="drug2">0.0</chemotactic_sensitivity>
                                <chemotactic_sensitivity substrate="substrate">0.0</chemotactic_sensitivity>
                            </chemotactic_sensitivities>
                        </advanced_chemotaxis>
                    </options>
                </motility>
                <secretion>
                    <substrate name="drug1">
                        <secretion_rate units="1/min">0</secretion_rate>
                        <secretion_target units="substrate density">0.0</secretion_target>
                        <uptake_rate units="1/min">0.1</uptake_rate>
                        <net_export_rate units="total substrate/min">0</net_export_rate>
                    </substrate>
                    <substrate name="drug2">
                        <secretion_rate units="1/min">0</secretion_rate>
                        <secretion_target units="substrate density">0.0</secretion_target>
                        <uptake_rate units="1/min">0</uptake_rate>
                        <net_export_rate units="total substrate/min">0</net_export_rate>
                    </substrate>
                </secretion>
                <cell_interactions>
                    <dead_phagocytosis_rate units="1/min">0</dead_phagocytosis_rate>
                    <live_phagocytosis_rates>
                        <phagocytosis_rate name="cancer" units="1/min">0</phagocytosis_rate>
                    </live_phagocytosis_rates>
                    <attack_rates>
                        <attack_rate name="cancer" units="1/min">0</attack_rate>
                    </attack_rates>
                    <damage_rate units="1/min">0</damage_rate>
                    <fusion_rates>
                        <fusion_rate name="cancer" units="1/min">0</fusion_rate>
                    </fusion_rates>
                </cell_interactions>
                <cell_transformations>
                    <transformation_rates>
                        <transformation_rate name="cancer" units="1/min">0</transformation_rate>
                    </transformation_rates>
                </cell_transformations>
            </phenotype>
            <custom_data>
                <simple_pressure conserved="false" units="dimensionless" description="">0.0</simple_pressure>
                <uptake_drug1 conserved="false" units="dimensionless" description="">0.0</uptake_drug1>
                <external_drug1 conserved="false" units="dimensionless" description="">0.0</external_drug1>
                <internal_drug1 conserved="false" units="dimensionless" description="">0.0</internal_drug1>
                <dna_damage conserved="false" units="dimensionless" description="">0.0</dna_damage>
                <uptake_drug2 conserved="false" units="dimensionless" description="">0.0</uptake_drug2>
                <external_drug2 conserved="false" units="dimensionless" description="">0.0</external_drug2>
                <internal_drug2 conserved="false" units="dimensionless" description="">0.0</internal_drug2>
                <spindle_damage conserved="false" units="dimensionless" description="">0.0</spindle_damage>
                <apoptosis_rate conserved="false" units="dimensionless" description="">0.15</apoptosis_rate>
                <rate_exit_M_phase conserved="false" units="dimensionless" description="">0.0</rate_exit_M_phase>
                <elapsed_time conserved="false" units="dimensionles" description="">0.0</elapsed_time>
            </custom_data>
        </cell_definition>
        <cell_rules>
            <rulesets>
                <ruleset protocol="CBHG" version="2.0" format="csv" enabled="false">
                    <folder>./config</folder>
                    <filename>cell_rules.csv</filename>
                </ruleset>
            </rulesets>
            <settings />
        </cell_rules>
    </cell_definitions>

    <initial_conditions>
        <cell_positions enabled="false" type="csv">
            <folder>./config</folder>
            <filename>cells.csv</filename>
        </cell_positions>
    </initial_conditions>

    <user_parameters>
        <random_seed type="int" units="dimensionless" description="">4158796556</random_seed>
        <population_fileName type="string" units="" description="name of file with phases population and volumes">PopCells_S000000_00.dat</population_fileName>
        <tumor_radius type="double" units="micron" description="initial radius of tumor">150</tumor_radius>
        <G0G1_initial_fraction type="double" units="micron" description="initial radius of cells in G0G1 phase">0.3462</G0G1_initial_fraction>
        <S_initial_fraction type="double" units="micron" description="initial fraction of cells in S phase">0.4097</S_initial_fraction>
        <G2_initial_fraction type="double" units="micron" description="initial fraction of cells in G2 phase">0.1782</G2_initial_fraction>
        <M_initial_fraction type="double" units="micron" description="initial fraction of cells in M phase"> 0.0432</M_initial_fraction>
        <apop_initial_fraction type="double" units="micron" description="initial fraction of apoptotic cells">0.0227</apop_initial_fraction>
        <chemotherapy_drug1 type="bool" units="dimensionless" description="activate chemotherapy with drug1 on x_min edge">true</chemotherapy_drug1>
        <time_chemotherapy_drug1 type="double" units="min" description="time to start the chemotherapy with drug1">0</time_chemotherapy_drug1>
        <duration_chemotherapy_drug1 type="double" units="min" description="duration of chemotherapy with drug1">0</duration_chemotherapy_drug1>
        <dose_chemotherapy_drug1 type="double" units="amount" description="dose of chemotherapy with drug1">0</dose_chemotherapy_drug1>
        <internal_saturation_drug1 type="double" units="[drug concentration]" description="drug1 saturation in each cell (internalized)">10.0</internal_saturation_drug1>
        <drug1_decay_rate type="double" units="1/min" description="drug1 decay rate in each cell (internalized)">0.1</drug1_decay_rate>
        <drug1_efflux_rate type="double" units="1/min" description="drug1 efflux rate in each cell">0.0</drug1_efflux_rate>
        <dna_damage_rate type="double" units="[DNA damage]*[min]/[drug amount]" description="rate of dna damage">1.0</dna_damage_rate>
        <dna_repair_rate type="double" units="1/min" description="repair rate of dna damage">0.01</dna_repair_rate>
        <dna_damage_apop_half_max type="double" units="dimensionless" description="half max of dna damage for apoptosis">0.5</dna_damage_apop_half_max>
        <dna_damage_arrest_half_max type="double" units="dimensionless" description="half max of dna damage for cell cycle arrest">0.0</dna_damage_arrest_half_max>
        <dna_damage_hill_coef type="double" units="dimensionless" description="hill coefficient of dna damage">4</dna_damage_hill_coef>
        <factor_max_apoptosis_drug1 type="double" units="dimensionless" description="factor that times base rate define max apoptosis rate drug1">100</factor_max_apoptosis_drug1>
        <factor_arrest_MtoG0G1_drug1 type="double" units="dimensionless" description="factor that times base rate define min MtoG0G1 rate drug1">0.5</factor_arrest_MtoG0G1_drug1>
        <chemotherapy_drug2 type="bool" units="dimensionless" description="activate chemotherapy with drug2 on x_min edge">false</chemotherapy_drug2>
        <time_chemotherapy_drug2 type="double" units="min" description="time to start the chemotherapy with drug1">0</time_chemotherapy_drug2>
        <duration_chemotherapy_drug2 type="double" units="min" description="duration of chemotherapy with drug1">0</duration_chemotherapy_drug2>
        <dose_chemotherapy_drug2 type="double" units="amount" description="dose of chemotherapy with drug1">0</dose_chemotherapy_drug2>
        <internal_saturation_drug2 type="double" units="[drug concentration]" description="drug2 saturation in each cell (internalized)">0.0</internal_saturation_drug2>
        <drug2_decay_rate type="double" units="1/min" description="drug2 decay rate in each cell (internalized)">0.0</drug2_decay_rate>
        <drug2_efflux_rate type="double" units="1/min" description="drug2 efflux rate in each cell">0.0</drug2_efflux_rate>
        <spindle_damage_rate type="double" units="[spindle damage]*[min]/[drug amount]" description="rate spindle damage">0.0</spindle_damage_rate>
        <spindle_repair_rate type="double" units="1/min" description="repair rate spindle damage">0.0</spindle_repair_rate>
        <spindle_damage_half_max type="double" units="dimensionless" description="half max of spindle damage">0</spindle_damage_half_max>
        <spindle_damage_hill_coef type="double" units="dimensionless" description="hill coefficient of spindle damage">0</spindle_damage_hill_coef>
        <factor_max_apoptosis_drug2 type="double" units="dimensionless" description="factor that times base rate define max apoptosis rate drug2">0</factor_max_apoptosis_drug2>
        <factor_arrest_MtoG0G1_drug2 type="double" units="dimensionless" description="factor that times base rate define min MtoG0G1 rate drug2">0</factor_arrest_MtoG0G1_drug2>
        <min_simple_pressure type="double" units="dimensionless" description="min simple pressure to down regulate proliferation">0.5</min_simple_pressure>
        <max_simple_pressure type="double" units="dimensionless" description="max simple pressure to down regulate proliferation">3.0</max_simple_pressure>
    </user_parameters>
</PhysiCell_settings>